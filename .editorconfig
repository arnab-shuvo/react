[*]
charset                     = utf-8
end_of_line                 = lf
indent_size                 = 3
indent_style                = tab
insert_final_newline        = true
max_line_length             = 120
tab_width                   = 3
ij_continuation_indent_size = 3
ij_formatter_off_tag        = @formatter: off
ij_formatter_on_tag         = @formatter: on
ij_formatter_tags_enabled   = false
ij_smart_tabs               = true
ij_visual_guides            = none
ij_wrap_on_typing           = false
